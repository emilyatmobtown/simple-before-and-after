{
    "name": "els/simple-before-and-after",
    "version": "0.1.1",
    "type": "wordpress-plugin",
    "keywords": ["wordpress", "plugin", "before-and-after", "grid", "image swapper"],
    "description": "Displays a simple grid that swaps Before and After images.",
    "authors": [
        {
            "name": "Emily Leffler Schulman",
            "email": "emily@mobtownstudios.com"
        }
    ],
    "require-dev": {
        "squizlabs/php_codesniffer": "dev-master",
        "dealerdirect/phpcodesniffer-composer-installer": "^0.5.0",
        "wp-coding-standards/wpcs": "^2.0",
        "phpcompatibility/phpcompatibility-wp": "^2.0",
        "els/els-php-standard": "dev-master",
        "roave/security-advisories": "dev-master",
        "phpunit/phpunit": "7.*"
    },
    "require": {
        "php": ">=7.0"
    },
    "scripts": {
        "lint": "vendor/bin/phpcs --standard=ELS-PHP-Standard .",
        "lint-fix": "vendor/bin/phpcbf --standard=ELS-PHP-Standard ."
    }
}
